<Window x:Class="WpfAutoPic.ReportWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAutoPic"
        mc:Ignorable="d"
        Title="ReportWin" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <FlowDocumentScrollViewer>
            <FlowDocument x:Name="flowDoc">
                        
            </FlowDocument>
        </FlowDocumentScrollViewer>
        <Button x:Name="buttonNyomtat" Content="Nyomtatás" Width="200" Margin="30" Grid.Row="1" Click="buttonNyomtat_Click" />
    </Grid>
</Window>
