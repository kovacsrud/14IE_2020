<Window x:Class="WpfKontenerek.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfKontenerek"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <StackPanel>
            <Button Content="Gomb"/>
            <Button Content="Gomb"/>
            <Button Content="Gomb"/>
            <Button Content="Gomb"/>
        </StackPanel>
        <WrapPanel Grid.Row="1">
            <Label x:Name="labelKiir" />
        </WrapPanel>
        <DockPanel Grid.Row="0" Grid.Column="1">
            <Button Content="Gomb1" DockPanel.Dock="Left" />
            <Button Content="Gomb2" DockPanel.Dock="Top"/>
            <Button Content="Gomb3" DockPanel.Dock="Bottom"/>
            <Button Content="Gomb4" DockPanel.Dock="Right"/>
        </DockPanel>
        <WrapPanel x:Name="elemek" Grid.Row="1" Grid.Column="1">
            
        </WrapPanel>
    </Grid>
</Window>
